    {
      "id": "format",
      "name": "Format Date",
      "description": "This module will format the output of 'Parse Date' to a given human readable form.",
      "hasReturn": true,
      "param": [
        {
          "param": "input",
          "displayName": "Parsed Date",
          "description": "Accepts an 'ISO-8601' standard string, or the response from the Parse Date module as input.",
          "type": "STRING"
        },
        {
          "param": "format",
          "displayName": "Format",
          "description": "The format you wish to retrieve from the module. Some formats have advanced options below.",
          "type": "ENUM",
          "value": [
            {
              "label": "ISO-8601 (YYYY-MM-DDTHH:mm:ssZ)",
              "value": "YYYY-MM-DDTHH:mm:ssZ"
            },
            "YYYY-MM-DD",
            "MM/DD/YYYY",
            "DD/MM/YYYY",
            "MM/DD",
            "DD/MM",
            "---",
            "HH:mm",
            "HH:mm:ss",
            "---",
            {
              "label": "Customized Format",
              "value": "CUSTOM"
            },
            {
              "label": "Relative Format",
              "value": "RELATIVE"
            }
          ]
        },
        {
          "param": "customized_format",
          "displayName": "Customized Format",
          "description": "Using a list of tokens to create a customized format.",
          "type": "STRING",
          "hideExpression": "(model.format && model.format.value && model.format.value !== \"CUSTOM\") || false"
        },
        {
          "param": "other_input",
          "displayName": "Reference Input",
          "description": "A comparison date. Accepts the same date format as the Parsed Date input. If no input is given then the time at execution will be used.",
          "type": "STRING",
          "hideExpression": "(model.format && model.format.value && model.format.value !== \"RELATIVE\") || false"
        }
      ]
    }
